<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/
Framework/View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Apptha_Marketplace::css/style.css" />
	</head>
	<body>
		<referenceBlock name="seller_account" remove="true" />
		<referenceBlock name="customer_account_navigation"
			remove="true" />
		<referenceContainer name="content">
			<block class="Apptha\Marketplace\Block\Seller\Displayseller"
				name="seller_content" template="seller/displayseller.phtml" />
			<block class="Apptha\Marketplace\Block\Seller\Sellerproducts"
				name="marketplace.allproducts" template="seller/allproducts.phtml"
				after="-" />
		</referenceContainer>
		<referenceContainer name="sidebar.main">
			<block class="Apptha\Marketplace\Block\Seller\Displayseller"
				name="seller_location" template="seller/map.phtml" />
			<block class="Apptha\Marketplace\Block\Seller\Displayseller"
				name="seller_ratings" template="seller/feedback.phtml" />
			<block class="Apptha\Marketplace\Block\Seller\Displayseller"
				name="seller_contact" template="seller/contact.phtml" />
		</referenceContainer>
	</body>
</page>